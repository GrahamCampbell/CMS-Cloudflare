{
    "name": "graham-campbell/cms-cloudflare",
    "description": "CMS CloudFlare Is A CMS Core Plugin That Adds A CloudFlare Admin Module",
    "keywords": ["laravel", "framework", "cms cloudflare", "cms", "cloudflare api", "CloudFlare", "CMS CloudFlare", "CMS-CloudFlare", "Graham Campbell", "GrahamCampbell"],
    "license": "AGPL-3.0",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "graham@mineuk.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/GrahamCampbell/dropbox-sdk-php"
        },
        {
            "type": "vcs",
            "url": "https://github.com/GrahamCampbell/asset"
        },
        {
            "type": "package",
            "package": {
                "name": "sebastianbergmann/phpcov",
                "version": "1.1.0",
                "dist": {
                    "url": "https://github.com/sebastianbergmann/phpcov/archive/1.1.0.zip",
                    "type": "zip"
                },
                "source": {
                    "url": "https://github.com/sebastianbergmann/phpcov.git",
                    "type": "git",
                    "reference": "1.1.0"
                },
                "bin": [
                    "phpcov.php"
                ]
            }
        }
    ],
    "require": {
        "php": ">=5.4.7",
        "laravel/framework": "4.1.*",
        "symfony/config": "2.4.*",
        "symfony/filesystem": "2.4.*",
        "graham-campbell/cms-core": "0.2.*",
        "graham-campbell/cloudflare-api": "0.3.*"
    },
    "require-dev": {
        "graham-campbell/testbench": "0.2.*"
    },
    "autoload": {
        "psr-0": {
            "GrahamCampbell\\CMSCloudFlare\\": "src/",
            "GrahamCampbell\\Tests\\CMSCloudFlare": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.2-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
